<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.tele2.digital.pojo.Redirect" table="redirects" catalog="REDIRECT_DB">
        <id name="id" type="int" column="id">
            <generator class="identity"/>
        </id>
        <property name="urlSource" type="java.lang.String" column="url_source"/>
        <property name="urlDestination" type="java.lang.String" column="url_destination"/>
        <property name="code" type="int" column="code"/>
        <property name="userAgent" type="java.lang.String" column="user_agent"/>
    </class>
</hibernate-mapping>